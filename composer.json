{
    "name": "jocoon/parquet",
    "description": "Thrift-based PHP implementation for using the Apache Parquet format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin",
            "email": "kevin@jocoon.de"
        }
    ],
    "require": {
        "php" : ">=7.3 <=7.4",
        "ext-gmp" : "*",
        "ext-snappy" : "*",
        "ext-bcmath" : "*",
        "ext-zlib": "*",
        "apache/thrift": "^0.13.0",
        "mdurrant/php-binary-reader": "^1.0",
        "nelexa/buffer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "jocoon\\parquet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jocoon\\parquet\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9 || 8 || 7"
    }
}
